
@{
    ViewData["Title"] = "Profile";
}

<h1>Profile</h1>
@{
    var mang = new List<int>();
    var rd = new Random();
    while(mang.Count < 6)
    {
        var tmp = rd.Next(1, 46);
        if(!mang.Contains(tmp)) mang.Add(tmp);
    }
}
<h2>KẾT QUẢ XỔ SỐ</h2>
@foreach(var item in mang)
{
    <div class="so">@item</div>
}
<style>
.so{width: 50px; height: 50px; border-radius: 25px; border: 2px solid red; display: inline-block; text-align: center;}
</style>
